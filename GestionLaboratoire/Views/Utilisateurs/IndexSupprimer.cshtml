@model IEnumerable<GestionLaboratoire.Models.Analyses>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Nom</th>
            <th>Prix</th>
            <th>Supprimer</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.AnalyseID</td>
                <td>@item.Nom</td>
                <td>@item.Prix</td>
                <td>
                    <a class="btn btn-danger" href="@Url.Action("SupprimerAnalyse", "Utilisateurs", new { id = item.AnalyseID })">Supprimer</a>
                </td>
            </tr>
        }
    </tbody>
</table>
