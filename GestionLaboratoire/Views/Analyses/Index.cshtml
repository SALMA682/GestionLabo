@model List<Analyses>
<h1>Liste des Analyses</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nom</th>
            <th>Prix</th>
            <th>Valeur Minimale</th>
            <th>Valeur Maximale</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var analyse in Model)
        {
            <tr>
                <td>@analyse.AnalyseID</td>
                <td>@analyse.Nom</td>
                <td>@analyse.Prix</td>
                <td>@analyse.ValeurMin</td>
                <td>@analyse.ValeurMax</td>
            </tr>
        }
    </tbody>
</table>